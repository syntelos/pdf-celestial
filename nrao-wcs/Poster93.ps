%!PS-Adobe-2.0
%%Creator: Eric Greisen
%%CreationDate: Sun 1993/1/12
%%Title: wcsPRO.ps
%%Pages: 1 1
%%DocumentFonts: Courier
%%BoundingBox: 0 0 480 420
%%EndComments

%%BeginProcSet: graphics.pro
   /TextFont /Courier-Bold findfont 15 scalefont def

 90 rotate
 1.5 1.4 scale
 200 0 translate

   % Constants.
   /cm2pts 72 2.54 div def
   /pi 3.1416 def
   /w1 0.4 def
   /w2 0.8 def
   /w4 1.6 def
   /nstr 13 string def

   % Radius of the generating sphere.
   /r0 2 cm2pts mul def
   /r1 r0 pi mul 180 div def

  /prt-n {
      dup abs 0.001 lt {pop 0} if
      nstr cvs show
      } def

  /BoxIt {
     dup stringwidth pop  /Xsp exch def
     /Ysp 15 def   /Xsp Xsp 8 add def
     currentpoint /Yc exch def /Xc exch def
     Xsp 2 div neg 0 rmoveto
     0 -2 Ysp mul rlineto
     Xsp 0 rlineto
     0 2 Ysp mul rlineto
     Xsp  neg 0 rlineto
     Xc Yc moveto
     Xsp 8 sub 2 div neg Ysp neg rmoveto show
     Xc Yc Ysp sub 1 sub moveto
     (COORDS) dup stringwidth pop 2 div neg -11 rmoveto show
     Xc Yc 2 Ysp mul sub moveto
     currentpoint /Yc exch def /Xc exch def
     stroke
     Xc Yc moveto
     } def

   /CenterIt {
      dup stringwidth pop /Xsp exch def
      Xsp -2 div -11 rmoveto
      show
      Xsp -2 div -4 rmoveto
      currentpoint /Yc exch def /Xc exch def
      stroke
      Xc Yc moveto
      } def

   /Arrow {
      -5 7 rmoveto
      5 -7 rlineto
      5 7 rlineto
      -5 -7 rmoveto
      } def

%%EndProcSet

%%EndProlog

userdict /start-hook known {start-hook} if


/vmsave save def
TextFont setfont
2 setlinecap

%% 170 410 translate

w4 setlinewidth

  0 0 moveto
  0 -20 rmoveto

  (ACTUAL PIXEL) BoxIt

  0 -20 rlineto
  (optional correction table) CenterIt
  0 -20 rlineto Arrow

  (IDEAL PIXEL) BoxIt

  0 -20 rlineto
  (linear transformation: multiply by) CenterIt
  197 17 rmoveto
  -42 -17 rlineto
  42 -17 rlineto
  3 21 rmoveto
  (CDn_m) show
  Xc 200 add Yc 11 sub moveto
  (CRPIXn) show
  Xc Yc moveto
  (CD matrix to scale, rotate, skew) CenterIt
  0 -20 rlineto Arrow

  (TRUE PLANE) BoxIt

  0 -20 rlineto
  (map projection per Section 4) CenterIt
  197 26 rmoveto
  -42 -17 rlineto
  42 -17 rlineto
  3 21 rmoveto
  (CTYPEn) show
  Xc 200 add Yc 4 sub moveto
  (PROJPi) show
  Xc Yc moveto
  0 -20 rlineto Arrow

  (NATIVE SPHERICAL) BoxIt

  0 -20 rlineto
  (rotation via equations 6) CenterIt
  197 26 rmoveto
  -42 -17 rlineto
  42 -17 rlineto
  3 21 rmoveto
  (CRVALn) show
  Xc 200 add Yc 4 sub moveto
  (LONGPOLE) show
  Xc Yc moveto
  0 -20 rlineto Arrow

  (CELESTIAL) BoxIt

%  ( Yc ) show Yc prt-n
vmsave restore
showpage


%%Trailer
userdict /end-hook known {end-hook} if
%%EOF
%%EndDocument
